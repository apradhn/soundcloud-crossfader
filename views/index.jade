extends layout

block content
  h1= title
  p Welcome to #{title}
  input(name="query" type="text")
  button#search Search
  .search-results
    ul
      li.user
  .tracks
    ul
      li.track
  .widget
    iframe(width='500', height='200', scrolling='no', frameborder='no', src='https://w.soundcloud.com/player/?url=https%3A//api.soundcloud.com/tracks/217890037&amp;auto_play=false&amp;hide_related=false&amp;show_comments=true&amp;show_user=true&amp;show_reposts=false&amp;visual=true')